{
    "name": "frosh/admin-sales-channel-search",
    "version": "1.0.0",
    "type": "shopware-platform-plugin",
    "keywords": [
        "admin",
        "saleschannel",
        "sales",
        "channel",
        "search",
        "shopware",
        "frosh"
    ],
    "description": "Sales channel search for Shopware 6 administration",
    "license": "MIT",
    "authors": [
        {
            "name": "FriendsOfShopware",
            "homepage": "https://friendsofshopware.de"
        }
