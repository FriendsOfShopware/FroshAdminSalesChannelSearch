        },
        "manufacturerLink": {
            "de-DE": "https://github.com/FriendsOfShopware/FroshAdminSalesChannelSearch",
            "en-GB": "https://github.com/FriendsOfShopware/FroshAdminSalesChannelSearch"
        },
        "supportLink": {
            "de-DE": "https://github.com/FriendsOfShopware/FroshAdminSalesChannelSearch/issues",
            "en-GB": "https://github.com/FriendsOfShopware/FroshAdminSalesChannelSearch/issues"
        }
